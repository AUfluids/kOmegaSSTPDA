// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          turbulenceProperties;
}

simulationType  RAS;

RAS
{
    RASModel        kOmegaSSTPDA;
    turbulence      on;
    printCoeffs     on;

   // Separation coefficients
   separationCorrection true;
   C0_2 1e-16;  // -2.070;
   C1_2 1e-16;  // 1.119;
   C2_2 1e-16;  // -0.215;
   C0_3 1e-16;  // -2.070;
   C1_3 1e-16;  // 1.119;
   C2_3 1e-16;  // -0.215;   
   lambda1 1; // 18.622;
   lambda2 1; // 4.698;

   // Anisotropy coefficients
   anisotropyCorrection true;
   A0_2 1e-16;  // -1.584;
   A1_2 1e-16;  // -0.685;
   A2_2 1e-16;  // -0.178;
   A0_3 1e-16;  // -1.584;
   A1_3 1e-16;  // -0.685;
   A2_3 1e-16;  // -0.178;
   anisotropyRelaxation 0.6;  // Relaxation factor for more stable simulations
}

